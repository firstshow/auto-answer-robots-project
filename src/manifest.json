{
  "name": "自动回复机器人",
  "version": "0.0.1",
  "manifest_version": 2,
  "author": "eleven",
  "description": "自动回复机器人",
  "action": {
    "default_popup": "index.html",
    "default_icon": "./resources/logo.png"
  },
  "icons": {
    "16": "./resources/logo.png",
    "48": "./resources/logo.png",
    "96": "./resources/logo.png",
    "128": "./resources/logo.png"
  },
  "browser_action": {
    "default_title": "自动评论机器人",
    "default_popup": "index.html/#/login"
  },
  "permissions": [
    "cookies",
    "tabs",
    "https://eos.douyin.com/*",
    "webRequest",
    "webRequestBlocking",
    "storage",
    "contextMenus"
  ]
}